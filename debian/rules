#!/usr/bin/make -f
THIS_SHOULD_GO_TO_UNSTABLE=1


UPSTREAMVERSION := 4:4.1.0

DEB_CMAKE_EXTRA_FLAGS += -DKDE4_ENABLE_EXPERIMENTAL_LIB_EXPORT=on

DEB_DH_MAKESHLIBS_ARGS_kdepimlibs5 := -V'kdepimlibs5 (>= $(UPSTREAMVERSION))'

# get rid of the dependency on kdebase-runtime
DEB_DH_SHLIBDEPS_ARGS_ALL:=-- -xkdebase-runtime

include debian/cdbs/kde.mk
